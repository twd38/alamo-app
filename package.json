  "type": "module",
  "private": true,
  "scripts": {
    "dev": "npm run sw:update && next dev --turbopack",
    "build": "npm run sw:update && next build",
    "start": "next start",
    "vercel-build": "prisma generate && npm run sw:update && next build",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "migrate:prod": "dotenv -e .env.production -- prisma migrate deploy",
    "db:check": "npx prisma db pull --print && echo 'Database connection successful'",
    "seed:rbac": "dotenv -e .env.local -- tsx scripts/seed-rbac.ts",
    "seed:rbac:prod": "dotenv -e .env.production -- tsx scripts/seed-rbac.ts",
    "seed:rbac:staging": "dotenv -e .env.staging -- tsx scripts/seed-rbac.ts",
    "seed:rbac:env": "dotenv -e .env.$NODE_ENV -- tsx scripts/seed-rbac.ts",
    "seed:mes": "dotenv -e .env.local -- tsx scripts/seed-mes.ts",
    "seed:mes:prod": "dotenv -e .env.production -- tsx scripts/seed-mes.ts",
    "init:admin": "dotenv -e .env.local -- tsx scripts/init-admin.ts",
    "init:admin:prod": "dotenv -e .env.production -- tsx scripts/init-admin.ts",
    "init:admin:staging": "dotenv -e .env.staging -- tsx scripts/init-admin.ts",
    "init:admin:env": "dotenv -e .env.$NODE_ENV -- tsx scripts/init-admin.ts",
    "create:test-user": "dotenv -e .env.local -- tsx scripts/create-test-user.ts",
    "typecheck": "tsc --noEmit",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "eslint --ext .js,.jsx,.ts,.tsx src/ --max-warnings 0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "check": "npm run format:check && npm run lint:strict && npm run build",
    "sw:update": "node scripts/update-sw-version.js",
    "deploy:update": "npm run sw:update && npm run build"
  },
